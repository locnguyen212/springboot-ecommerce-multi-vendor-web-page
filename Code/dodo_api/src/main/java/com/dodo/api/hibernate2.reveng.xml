  <table-filter match-catalog="dodo_api" match-name="productattribute"/>
  <table-filter match-catalog="dodo_api" match-name="promotion"/>
  <table-filter match-catalog="dodo_api" match-name="review"/>
  <table-filter match-catalog="dodo_api" match-name="role"/>
  <table-filter match-catalog="dodo_api" match-name="salesdata"/>
  <table-filter match-catalog="dodo_api" match-name="shopowner"/>
